{
  "name": "Set Solver",
  "version": "1.0",
  "description": "Let us solve your game of Set with Friends and win some internet points.",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
